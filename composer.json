{
    "name": "devnile/microboard",
    "description": "This package creates to decrease working time that spent on admin panels",
    "homepage": "https://github.com/devnile/microboard",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Ibrahem",
            "email": "mohamed.ibrahem@devnile.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Microboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Microboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microboard\\MicroboardServiceProvider"
            ]
        }
    }
}
